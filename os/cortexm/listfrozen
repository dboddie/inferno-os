$AWK '
BEGIN{
    path_re="/[^.]+.fdis$"
    suffix_re=".fdis$"
    printf("MODOBJ=")
}
$2 ~ path_re {
    fpath=$2
    # Remove the suffix and convert slashes into underscores.
    prefix=fpath
    gsub(suffix_re, "", prefix)
    gsub("/", "_", prefix)
    prefix=substr(prefix, 1)

    printf("frozen/"prefix".$O ")
}
' $CONF
